{
  "Name": "${ORTHANC__NAME}",
  "StorageDirectory": "/var/lib/orthanc/storage",
  "IndexDirectory": "/var/lib/orthanc/index",
  "DicomAet": "${ORTHANC__DICOM_AET}",
  "DicomPort": 4242,
  "HttpPort": 8042,
	"DicomModalities": {
		"adbouRadiant": [ "ADBOU-RADIANT", "host.docker.internal", 11112 ]
	},
  "RemoteAccessAllowed": true,
  "AuthenticationEnabled": true,
  "SslEnabled": false,
  "CompressionEnabled": true,
	"OrthancExplorer2" : {
		"Enabled": true,
		"IsDefaultOrthancUI" : true
	},
  "PostgreSQL": {
    "EnableIndex": true,
    "EnableStorage": false,
    "Host": "postgres",
    "Port": 5432,
    "Database": "${POSTGRES_DB}",
    "Username": "${POSTGRES_USER}",
    "Password": "${POSTGRES_PASSWORD}",
    "IndexConnectionsCount": 10,
    "Lock": true
  },
  "RestApiWritePermissions": true,
  "HttpDescribeErrors": false,
  "HttpServerThreadsCount": 8,
  "JobsEngineThreadsCount" : {
    "ResourceModification": 4   
		},
  "StrictAetComparison": true,
  "DicomAssociationCloseDelay": 2,
  "OverwriteInstances": false,
  "UnknownSopClassAccepted": false,
  "LimitJobs": 20,
  "DicomScpTimeout": 30,
  "CaseSensitivePN": false
}
